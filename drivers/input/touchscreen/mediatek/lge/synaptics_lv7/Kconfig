if INPUT_TOUCHSCREEN

menuconfig LGE_TOUCH_SYNAPTICS_TD4100_LV7
	bool "LGE Touch Core ODM Device Driver - Synaptics(td4100_lv7)"
	depends on LGE_TOUCH_CORE_BASE
	help
	  If your device is synaptics(td4100_lv7), say Y

menuconfig LGE_TOUCH_TOVIS_SYNAPTICS_TD4100
	bool "LGE Touch Core ODM Device Driver - Tovis Synaptics(td4100)"
	depends on LGE_TOUCH_CORE_BASE
	help
	  If your device is tovis synaptics(td4100), say Y

endif
