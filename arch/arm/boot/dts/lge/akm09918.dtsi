cust_mag@c {
    compatible = "mediatek,akm09918";
    reg = <0x0c>;
    status = "okay";
    i2c_num = <1>;
    i2c_addr = <0x0c 0 0 0>;
    direction = <8>;
    power_id = <0xffff>;
    power_vol = <0>;
    is_batch_supported = <0>;
};
